[{"ratings": {"rAmountLearned": "2.25", "rDifficulty": "2.07", "rStimulateInterest": "1.93", "rRecommendMajor": "3.18", "rCourseQuality": "2.10", "rWorkRequired": "1.98", "rInstructorAccess": "2.63", "rReadingsValue": "1.58", "rRecommendNonMajor": "0.79", "rCommAbility": "2.25", "rInstructorQuality": "2.20"}, "section": {"name": "CONCEPTS IN NURSING: HEALTHY LIFE STYLES I", "sectionnum": "001", "primary_alias": "NURS-104-001", "path": "/courses/24850/sections/001", "id": "24850-001", "aliases": ["NURS-104-001"]}, "comments": "This introductory clinical course deals with health promotion and disease\r\nprevention with healthy and at-risk individuals in the community. Students address the\r\ntheoretical component of the course in weekly seminars. The clinical component focuses\r\non the communication techniques and basic clinical skills and technologies used to assess\r\nhealth status, promote health and prevent illness. Students then integrate theoretical\r\nconcepts and clinical skills and apply them in a variety of community settings, focusing\r\non health promotion and disease prevention with healthy and at-risk individuals.\r\nFeelings toward the content of this course were mixed. Some students found the\r\ntopics interesting, but others remarked that the material was \u201cpainfully simple.\u201d\r\nThe guest lecturers were generally praised, but one student complained about the\r\noverall dependence on powerpoints. Other students spoke of a lack of clarity in the\r\nexpectations of both Professor George and Professor Quigley.\r\nStudents were surprised by the final exam, which several defined as confusing,\r\nand the strict grading in this course.", "num_students": 61, "path": "/courses/24850/sections/001/reviews/4478-MAUREEN-GEORGE", "instructor": {"path": "/instructors/4478-MAUREEN-GEORGE", "first_name": "MAUREEN", "last_name": "GEORGE", "id": "4478-MAUREEN-GEORGE", "name": "MAUREEN GEORGE"}, "num_reviewers": 61, "id": "24850-001-4478-MAUREEN-GEORGE"}, {"ratings": {"rAmountLearned": "2.59", "rDifficulty": "2.18", "rStimulateInterest": "2.64", "rRecommendMajor": "3.25", "rCourseQuality": "2.56", "rWorkRequired": "1.94", "rInstructorAccess": "3.30", "rReadingsValue": "1.85", "rRecommendNonMajor": "0.82", "rCommAbility": "3.15", "rInstructorQuality": "3.19"}, "section": {"name": "CONCEPTS IN NURSING: HEALTHY LIFE STYLES I", "sectionnum": "002", "primary_alias": "NURS-104-002", "path": "/courses/24850/sections/002", "id": "24850-002", "aliases": ["NURS-104-002"]}, "comments": "Nursing 104 is an introductory clinical course that deals with health promotion and disease prevention with healthy and at-risk individuals in the community. Students will address the theoretical component of the course in weekly seminars. The clinical component focuses on the communication techniques and basic clinical skills and technologies used to assess health status, promote health and prevent illness. Students then integrate theoretical concepts and clinical skills and apply them in a variety of community settings, focusing on health promotion and disease prevention with healthy and at-risk individuals.\r\n\r\nFeelings toward the content of this course were mixed. Some students found the topics interesting, but others remarked that the material was \u201cpainfully simple.\u201d\r\n\r\nThe guest lecturers were generally praised, but one student complained about the overall dependence on PowerPoint presentations. Other students spoke of a lack of clarity in the expectations of both Professor George and Professor Quigley.\r\n\r\nStudents were surprised by the strict grading in the course and by the final exam, which several defined as confusing.\r\n", "num_students": 37, "path": "/courses/24850/sections/002/reviews/2237-BETH-QUIGLEY", "instructor": {"path": "/instructors/2237-BETH-QUIGLEY", "first_name": "BETH", "last_name": "QUIGLEY", "id": "2237-BETH-QUIGLEY", "name": "BETH QUIGLEY"}, "num_reviewers": 36, "id": "24850-002-2237-BETH-QUIGLEY"}, {"