[{"ratings": {"rAmountLearned": "3.53", "rDifficulty": "2.73", "rTAQuality": "2.99", "rStimulateInterest": "3.78", "rRecommendMajor": "3.74", "rCourseQuality": "3.42", "rWorkRequired": "2.82", "rInstructorAccess": "3.81", "rReadingsValue": "3.01", "rRecommendNonMajor": "1.03", "rCommAbility": "3.76", "rInstructorQuality": "3.85"}, "section": {"name": "NURSING OF CHILDREN: THEORY/CLINICAL", "sectionnum": "001", "primary_alias": "NURS-220-001", "path": "/courses/27872/sections/001", "id": "27872-001", "aliases": ["NURS-220-001"]}, "comments": "Using developmental and systems frameworks and a family centered approach, the health and illness needs of children from birth through adolescence, and the related needs of their caregivers are addressed. The student is provided with a theory base, as well as opportunities for providing care to children with acute and chronic health problems, and their families. Using critical thinking, including the nursing process, strategies are developed for optimizing health; maintaining individuality; promoting optimal developmental, physiological, and psychological functioning; and, for enhancing strengths within the family unit. There are three primary foci for this course: taking care of sick children and families, child development, and professionalism.\r\n\r\n\tThis course received overwhelmingly positive reviews, with students raving that it was \u201cexcellent,\u201d \u201cwell-run,\u201d and one of the best courses in both the clinical program and the nursing school overall.\r\n\r\n\tThe faculty lecturers were energetic, caring, organized, \u201cthe best.\u201d The guest lecturers were also good.\r\n\r\n\r\nPrerequisites include NURS 104, 106, 131, and 132. \r\n", "num_students": 88, "path": "/courses/27872/sections/001/reviews/2600-KIRSTEN-HICKERSON", "instructor": {"path": "/instructors/2600-KIRSTEN-HICKERSON", "first_name": "KIRSTEN", "last_name": "HICKERSON", "id": "2600-KIRSTEN-HICKERSON", "name": "KIRSTEN HICKERSON"}, "num_reviewers": 77, "id": "27872-001-2600-KIRSTEN-HICKERSON"}, {"ratings": {"rAmountLearned": "3.53", "rDifficulty": "2.73", "rTAQuality": "2.99", "rStimulateInterest": "3.40", "rRecommendMajor": "3.74", "rCourseQuality": "3.42", "rWorkRequired": "2.82", "rInstructorAccess": "3.41", "rReadingsValue": "3.01", "rRecommendNonMajor": "1.03", "rCommAbility": "3.44", "rInstructorQuality": "3.31"}, "section": {"name": "NURSING OF CHILDREN: THEORY/CLINICAL", "sectionnum": "001", "primary_alias": "NURS-220-001", "path": "/courses/27872/sections/001", "id": "27872-001", "aliases": ["NURS-220-001"]}, "comments": "Using developmental and systems frameworks and a family centered approach, the health and illness needs of children from birth through adolescence, and the related needs of their caregivers are addressed.  The student is provided with a theory base, as well as opportunities for providing care to children with acute and chronic health problems, and their families.  Using critical thinking, including the nursing process, strategies are developed for optimizing health; maintaining individuality; promoting optimal developmental, physiological, and psychological functioning; and, for enhancing strengths within the family unit. There are three primary foci for this course: taking care of sick children and families, child development, and professionalism.\r\n\r\nStudents felt the course was well-organized with \u201cexcellent support from the faculty.\u201d  All the instructors were lauded for their commitment to the class and ensuring that it was properly organized.\r\n\r\nStudents praised all of the instructors equally for exerting a great deal of enthusiasm and time to the benefit of the class. \r\n\r\nMany of the students felt this was \u201cthe best core clinical class\u201d and should act as a model for \u201call the clinical courses at Penn.\u201d  Students praised highly the coordination among different lecturers, and they enjoyed the diversity of perspectives from the various professors.\r\n", "num_students": 88, "path": "/courses/27872/sections/001/reviews/5464-SHARON-Y-IRVING", "instructor": {"path": "/instructors/5464-SHARON-Y-IRVING", "first_name": "SHARON Y", "last_name": "IRVING", "id": "5464-SHARON-Y-IRVING", "name": "SHARON Y IRVING"}, "num_reviewers": 77, "id": "27872-001-5464-SHARON-Y-IRVING"}, {"ratings": {"rAmountLearned": "4.00", "rDifficulty": "3.20", "rTAQuality": "3.50", "rStimulateInterest": "4.00", "rRecommendMajor": "3.80", "rCourseQuality": "4.00", "rWorkRequired": "3.60", "rInstructorAccess": "4.00", "rReadingsValue": "3.60", "rRecommendNonMajor": "1.60", "rCommAbility": "4.00", "rInstructorQuality": "4.00"}, "section": {"name": "CLINIC: CHOP - 4W CSH", "sectionnum": "101", "primary_alias": "NURS-220-101", "path": "/courses/27872/sections/101", "id": "27872-101", "aliases": ["NURS-220-101"]}, "comments": "", "num_students": 7, "path": "/courses/27872/sections/101/reviews/6141-JILLIAN-BETH-AUFSEESER", "instructor": {"path": "/instructors/6141-JILLIAN-BETH-AUFSEESER", "first_name": "JILLIAN BETH", "last_name": "AUFSEESER", "id": "6141-JILLIAN-BETH-AUFSEESER", "name": "JILLIAN BETH AUFSEESER"}, "num_reviewers": 5, "id": "27872-101-6141-JILLIAN-BETH-AUFSEESER"}, {"ratings": {"rAmountLearned": "3.33", "rDifficulty": "3.17", "rTAQuality": "4.00", "rStimulateInterest": "2.83", "rRecommendMajor": "3.00", "rCourseQuality": "2.80", "rWorkRequired": "3.67", "rInstructorAccess": "3.50", "rReadingsValue": "1.75", "rRecommendNonMajor": "0.00", "rCommAbility": "3.00", "rInstructorQuality": "3.17"}, "section": {"name": "CLINIC: CHOP - 5 West", "sectionnum": "102", "primary_alias": "NURS-220-102", "path": "/courses/27872/sections/102", "id": "27872-102", "aliases": ["NURS-220-102"]}, "comments": "", "num_students": 7, "path": "/courses/27872/sections/102/reviews/5827-SUSAN-BIRKHOFF", "instructor": {"path": "/instructors/5827-SUSAN-BIRKHOFF", "first_name": "SUSAN", "last_name": "BIRKHOFF", "id": "5827-SUSAN-BIRKHOFF", "name": "SU